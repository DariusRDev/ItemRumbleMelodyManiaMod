#itemCountLabel {
    text-shadow: 1px 1px 0 rgba(0, 0, 0, 0.5);
    font-size: 14px;
    -unity-font-style: bold;
}

#itemCollectorItem {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 16px;
    height: 16px;
    margin-left: -8px;
    margin-top: -8px;
}

#itemCountContainer {
    position: absolute;
    top: 70%;
    left: 90%;
    width: 20px;
    height: 20px;
    align-items: center;
    justify-content: center;
}